//API8BPL JOB 'BINDPLAN API8',MSGCLASS=H,CLASS=A,
//           REGION=4M,MSGLEVEL=(1,1),NOTIFY=&SYSUID,COND=(4,LT),
//           RESTART=*,TIME=(0,5)
//*====================================================================
//* BIND PLAN
